<?xml version="1.0"?>
<sdf version="1.5">
  <model name="parrot_bebop_2">
    <static>false</static>
    <pose>0 0 0.062656 0 0 0</pose>

    <!-- Main Body Link -->
    <link name="body">
      <inertial>
        <mass>0.503</mass>
        <inertia>
          <ixx>1.805e-3</ixx>
          <ixy>-8.619e-7</ixy>
          <iyy>1.764e-3</iyy>
          <ixz>1.555e-5</ixz>
          <iyz>3.595e-7</iyz>
          <izz>3.328e-3</izz>
        </inertia>
      </inertial>

      <!-- Add Collision -->
      <collision name="body_collision">
        <geometry>
          <box>
            <size>0.3 0.3 0.1</size> <!-- Approximate size of the drone body -->
          </box>
        </geometry>
      </collision>

      <visual name="hull">
        <geometry>
          <mesh>
            <uri>model://parrot_bebop_2/meshes/hull.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>

    <!-- Front-Left Propeller -->
    <link name="propeller_fl">
      <pose>0.08456 0.11463 -0.00501 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <iyy>1e-5</iyy>
          <izz>1e-5</izz>
        </inertia>
      </inertial>

      <!-- Add Collision -->
      <collision name="propeller_fl_collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.01</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="propeller_fl_visual">
        <geometry>
          <mesh>
            <uri>model://parrot_bebop_2/meshes/propeller_fl.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="propeller_fr">
      <pose>0.08456 -0.11435 -0.00501 0 0 0</pose>
