          <cylinder>
            <radius>0.05</radius>
            <length>0.01</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="propeller_fr_visual">
        <geometry>
          <mesh>
            <uri>model://parrot_bebop_2/meshes/propeller_fr.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="propeller_rl">
      <pose>-0.09035 0.11452 -0.00501 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <iyy>1e-5</iyy>
