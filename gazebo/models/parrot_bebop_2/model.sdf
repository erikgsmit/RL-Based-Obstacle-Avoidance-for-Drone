          </mesh>
        </geometry>
      </visual>
    </link>

    <!-- Joints -->
  

     <joint name="joint_propeller_fl" type="revolute">
      <parent>body</parent>
      <child>propeller_fl</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-3.14</lower>
          <upper>3.14</upper>
          <effort>1e6</effort>
        </limit>
        <dynamics>
          <damping>0.01</damping>
          <friction>0.001</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="joint_propeller_fr" type="revolute">
